<h1>Agendas for all attendees at <%=h @context.fullname %></h1>

<table style="width: 100%">
  <% @attendees.each do |attendee| -%>
    <tr><td colspan="4"><h2><%= attendee.lastname %>, <%= attendee.firstname %></td></tr>

    <%= render :partial => 'attendance', :collection => @attendances[attendee] %>
  <% end -%>
</table>
